<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="interviewDao" class="com.mym.homework.dao.InterviewParser"/>

    <bean id="interviewService" class="com.mym.homework.service.InterviewProcess">
        <constructor-arg name="fileName" value="/questions.csv"/>
        <constructor-arg name="delimiter" value=","/>
        <constructor-arg name="interviewParser" ref="interviewDao"/>
    </bean>
</beans>
